<div class="morphbutton">
  <button type="button" class="btn btn-primary morphbutton-open" style="display: none;">Open</button>
  <div class="morphbutton-content" style="top: 234px; left: 0px; width: 60px; height: 34px; opacity: 0;">
    <button type="button" class="morphbutton-close"><span class="glyphicon glyphicon-remove"></span></button>
    <form class="form-horizontal" role="form">
      <fieldset>

        <!-- Form Name -->
        <legend>Campaign Details</legend>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-sm-2 control-label" for="textinput">desc</label>
          <div class="col-sm-10">
            <input name="camp_desc" type="text" class="form-control">
          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-sm-2 control-label" for="textinput">title</label>
          <div class="col-sm-10">
            <input name='camp_title' type="text" placeholder="camp_title" class="form-control">
          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-sm-2 control-label" for="textinput">url</label>
          <div class="col-sm-10">
            <input name='camp_url' type="text" placeholder="camp_url" class="form-control">
          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-sm-2 control-label" for="textinput">start</label>
          <div class="col-sm-10">
            <input name='start_date' type="text" placeholder="start_date" class="form-control datepicker">
          </div>
          <label class="col-sm-2 control-label" for="textinput">end</label>
          <div class="col-sm-10">
            <input name='end_date' type="text" placeholder="end_date" class="form-control datepicker">
          </div>
        </div>

        <!-- Text input-->
        <div class="form-group">
          <label class="col-sm-2 control-label" for="textinput">expire day</label>
          <div class="col-sm-4">
            <input name='ad_expire_day' type="text" placeholder="ad_expire_day" class="form-control">
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-offset-2 col-sm-10">
            <div class="pull-right">
              <button type="submit" class="btn btn-default">Cancel</button>
              <button type="submit" class="btn btn-primary">Save</button>
            </div>
          </div>
        </div>
      </fieldset>
    </form>
  </div>
</div>
